{
  "name": "2018.mirrorconf.com",
  "description": "MirrorConf's 2018 landing page",
  "version": "1.0.0",
  "author": "Subvisual <contact@subvisual.co>",
  "dependencies": {
    "@jacobmischka/gatsby-plugin-react-svg": "^1.1.0",
    "classnames": "^2.2.5",
    "gatsby": "^1.9.178",
    "gatsby-link": "^1.6.34",
    "gatsby-plugin-google-analytics": "^1.0.31",
    "gatsby-plugin-postcss-sass": "^1.0.19",
    "gatsby-plugin-react-css-modules": "^1.0.11",
    "gatsby-plugin-react-helmet": "^2.0.3",
    "gatsby-plugin-react-svg": "^1.1.1",
    "lodash": "^4.17.5",
    "pixi.js": "^4.7.3",
    "popmotion": "^8.1.14",
    "react-helmet": "^5.2.0",
    "style-value-types": "^1.0.4",
    "webpack": "^1.15.0"
  },
  "keywords": [
    "mirroconf"
  ],
  "license": "UNLICENSED",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "develop": "concurrently -k -n \"GATSBY,STORYBOOK\" \"gatsby develop -H 0.0.0.0\" \"start-storybook -p 9001 -c .storybook\"",
    "format": "prettier --trailing-comma es5 --single-quote --write \"src/**/*.js\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "deploy-staging": "gatsby build --prefix-paths && gh-pages -d public",
    "deploy-production": "gatsby build && s3-deploy './public/**' --cwd './public/' --bucket mirrorconf.com --deleteRemoved --signatureVersion v4 --distId E3LFA5PNA9AVTY"
  },
  "devDependencies": {
    "@storybook/addon-info": "^3.3.13",
    "@storybook/addon-options": "^3.3.13",
    "@storybook/react": "^3.3.13",
    "concurrently": "^3.5.1",
    "gh-pages": "^1.1.0",
    "identity-obj-proxy": "^3.0.0",
    "imports-loader": "^0.7.1",
    "postcss-custom-properties": "^7.0.0",
    "prettier": "^1.10.2",
    "react-router-dom": "^4.2.2",
    "s3-deploy": "^1.0.0",
    "storybook-addon-a11y": "^3.1.9"
  }
}
